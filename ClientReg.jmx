            <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy;
import org.apache.jmeter.protocol.http.util.HTTPArgument;
import org.apache.jmeter.testelement.property.CollectionProperty;
import org.apache.jmeter.testelement.property.JMeterProperty;

// Retrieve the current HTTP sampler
HTTPSamplerProxy sampler = ctx.getCurrentSampler();
log.info(&quot;Sampler retrieved: &quot; + sampler);

// Retrieve the request body arguments
CollectionProperty arguments = sampler.getArguments().getArguments();
for (JMeterProperty prop : arguments) {
    HTTPArgument argument = (HTTPArgument) prop.getObjectValue();
    log.info(&quot;Request parameter: &quot; + argument.getName() + &quot; = &quot; + argument.getValue());
    vars.put(argument.getName(), argument.getValue());
    
}
</stringProp>
