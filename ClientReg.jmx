          <stringProp name="script">def randomName = org.apache.commons.lang3.RandomStringUtils.randomAlphabetic(8);
def randomUsername = org.apache.commons.lang3.RandomStringUtils.randomAlphabetic(8);
def randomEmail = org.apache.commons.lang3.RandomStringUtils.randomAlphabetic(8) + &quot;@test.com&quot;;
def randomPassword = org.apache.commons.lang3.RandomStringUtils.randomAlphanumeric(12);
def randomPhone = &quot;+971&quot; + org.apache.commons.lang3.RandomStringUtils.randomNumeric(9);

vars.put(&quot;randomName&quot;, randomName);
vars.put(&quot;randomUsername&quot;, randomUsername);
vars.put(&quot;randomEmail&quot;, randomEmail);
vars.put(&quot;randomPassword&quot;, randomPassword);
vars.put(&quot;randomPhone&quot;, randomPhone);</stringProp>
